<div>
  <h2>入力情報の確認</h2>

 <div class='confirmation'>
   <p>お名前<br /> <%= @user.username %></p>
   <p>お名前(カナ)<br /> <%= @user.username_kana %></p>
   <p>住所<br /> 〒<%= @user.postal_code %><br /><%= @user.prefecture_code %><br /><%= @user.city %><br /><%= @user.street_and_others %><br /></p>
   <p>電話番号<br /> <%= @user.tel %></p>
   <p>メールアドレス<br /> <%= @user.email %></p>
   <p>パスワード<br /> <%= @password %><br /><%= @password_confirmation %></p>
   <p>生年月日<br /> <%= @user.birthday %></p>
   <p>性別<br /> <%= @user.sex %></p>
   <p>職業<br /> <%= @user.job %></p>
 </div>

 <%# <%= form_with model: @user, url: users_sign_up_complete_path(@user), local: true do |f| %> 
 <%= form_for(resource, as: resource_name, url: users_sign_up_complete_path(resource_name)) do |f| %>
    <%= f.hidden_field :username %>
    <%= f.hidden_field :username_kana %>
    <%= f.hidden_field :postal_code %>
    <%= f.hidden_field :prefecture_code %>
    <%= f.hidden_field :city %>
    <%= f.hidden_field :street_and_others %>
    <%= f.hidden_field :tel %>
    <%= f.hidden_field :email %>
    <%= f.hidden_field :password %>
    <%= f.hidden_field :password_confirmation %>
    <%= f.hidden_field :birthday %>
    <%= f.hidden_field :sex %>
    <%= f.hidden_field :job %>

    <%= f.submit "戻る", name: :back %>
    <%= f.submit "登録する" %>
 <% end %>
</div>